		<div class='song-w dark its-a'>
			<div class='lyric'>
				{{#fit-text min='20px' max='600px' compressor='1'}}
					<span>Its a</span>
					<span>nice day</span>
					<span>for a</span>
					<span>white</span>
					<span>wedding</span>
				{{/fit-text}}
			</div>
		</div>

	{{/adopt-orphans}}

	<footer class='cta'>
		<div class='text-w'>
			{{#link-to 'contact' class='button inquire'}}
				<span>Inquire</span>
			{{/link-to}}
			{{#link-to 'faqs' class='button faq'}}
				<span>FAQs</span>
			{{/link-to}}
		</div>
	</footer>

</div>
</section>



{{outlet}}
