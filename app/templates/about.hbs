				{{/fit-text}}
			</div>
		</div>

{{!-- 		<div class='image-w'>
			<img src='{{model.acf.about_song_2.sizes.large}}' alt=''>
		</div> --}}

		<div class='image-w cake'>
			<img src='{{model.acf.about_image_4.sizes.large}}' alt=''>
		</div>

		<div class='text-w'>
			{{{model.acf.about_character}}}
		</div>




		<div class='image-w pillow'>
			<img src='{{model.acf.about_image_5.sizes.large}}' alt=''>
		</div>

		<div class='text-w'>
			{{{model.acf.about_press}}}
		</div>

		<div class='image-w pool'>
			<img src='{{model.acf.about_image_6.sizes.large}}' alt=''>
		</div>

		<div class='text-w'>
			{{{model.acf.about_intro_4}}}
